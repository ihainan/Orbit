# Database password (required)
DB_PASSWORD=your_password_here

# Optional: override database user/name
# DB_USER=orbitals
# DB_NAME=orbitals_db

# Default user — created automatically on first run if no user exists in the database
DEFAULT_USERNAME=your_username_here
DEFAULT_EMAIL=your_email_here

# AMap (高德地图) Web Service API Key
# Obtain from: https://console.amap.com/
AMAP_WEB_SERVICE_KEY=your_amap_api_key_here

# Persistent data paths (relative to docker/prod/)
POSTGRES_DATA_DIR=./data/postgres
UPLOADS_DIR=./data/uploads

# Port exposed by the nginx container on the host
HTTP_PORT=80
